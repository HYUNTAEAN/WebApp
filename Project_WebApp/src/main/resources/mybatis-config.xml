<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="mapUnderscoreToCamelCase" value="true" />
	</settings>

	<typeAliases>
		<typeAlias type="net.scit.webapp.vo.UserVO" alias="User" />
		<typeAlias type="net.scit.webapp.vo.BookmarkVO" alias="bookmarkVO" />
		<typeAlias type="net.scit.webapp.vo.CommandVO" alias="command"/>
	</typeAliases>

	<mappers>
		<mapper resource="/mappers/appMapper.xml" />
	</mappers>
</configuration>

